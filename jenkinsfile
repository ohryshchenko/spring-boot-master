
pipeline {
agent any
tools {
    maven 'Maven 3.3.9'
    jdk 'jdk8'
}

stage ("initialize") {
steps {
sh '''
echo "PATH = ${PATH}"
echo "M2_HOME = ${M2_HOME}"
'''
  }
                  }


                  stage('Build') {
                      withMaven(maven: 'Maven 3') {
                        dir('spring-boot-tests/spring-boot-smoke-tests/spring-boot-smoke-test-web-ui/') {
                          sh 'mvn clean package'
                          
                        }
                      }
                    }

}
